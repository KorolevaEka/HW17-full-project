    function setCookie(name, value, days){
        var date = new Date();
        date.setTime(date.getTime() + (days*24*60*60*1000)); 
        var expires = "; expires=" + date.toGMTString();
        document.cookie = name + "=" + value + expires + ";path=/";
    }
    function getParam(p){
        var match = RegExp('[?&]' + p + '=([^&]*)').exec(window.location.search);
        return match && decodeURIComponent(match[1].replace(/\+/g, ' '));
    }
    var yclid = getParam('yclid');
    if(yclid){
        setCookie('yclid', yclid, 90);
    }
</script><script>
   window.DATA_LAYER = (function(){
      var listeners = [];
      window.dataLayer = window.dataLayer || [];
      window.dataLayer.push = function (value) {
         for(var i = 0; i < listeners.length; i++) listeners[i](value);
         return Array.prototype.push.apply(window.dataLayer, arguments);
      }
      return {
         subscribe: function(notifyCallback){listeners.push(notifyCallback);},
      };
   })();
</script>
    <script>
        function readCookie(name) { 
          var n = name + "="; 
          var cookie = document.cookie.split(';'); 
          for(var i=0;i < cookie.length;i++) {      
              var c = cookie[i];      
              while (c.charAt(0)==' '){c = c.substring(1,c.length);}      
              if (c.indexOf(n) == 0){return c.substring(n.length,c.length);} 
          } 
          return null; 
          } 
          
         var yid = readCookie('_ym_uid');
         var time = new Date().getTime();
         var platform_Id = readCookie('platformId');
         var ycl_id = readCookie('yclid');
         
         //параметры визита и параметры пользователя
         ym(16914655, 'params', {
                 SessionID_YM: time + "." + yid,
                 platformId: platform_Id,
                 yclid: ycl_id,
                 ClientID_YM: yid
         });
         
         
